// same_prefix(a, b) => is_valid_email(a) <=> is_valid_email(b)
0.9  !same_prefix(a, b) v !is_valid_email(a) v is_valid_email(b)
0.9  !same_prefix(a, b) v !is_valid_email(b) v is_valid_email(a)

// same_domain(a, b) => is_valid_email(a) <=> is_valid_email(b)
0.5  !same_domain(a, b) v is_valid_email(a) v is_valid_email(b)

// same_address(a, b) => is_valid_email(a) <=> is_valid_email(b)
